#include <bits/stdc++.h>
using namespace std;

class student {
private:
    string name;
    int age;
    string grade;

public:
    void getname(string s) {
        if (s.size() == 0) {
            cout << "invalid";
            return;
        }
        name = s;
    }
    void setname() {
        cout << name << endl;
    }
    void getage(int a) {
        age = a;
        if (age <= 0 || age > 100) {
            cout << "invalid";
            return;
        }
    }
    void setage() {
        cout << age << endl;
    }

    void getgrade(string g) {
        grade = g;
    }
    void setgrade(int pin) {
        if (pin == 123) {
            cout << grade << endl;
        } else {
            cout << "Access Denied" << endl;
        }
    }
};

int main() {
    student s1;
    s1.getname("Vaish");
    s1.setname();
    s1.getage(10);
    s1.setage();
    s1.getgrade("A+");
    s1.setgrade(123); 
    return 0;
}
